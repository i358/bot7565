{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "Discord bot with TypeScript",
  "main": "dist/bin.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/bin.js",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/bin.js",
    "dev": "concurrently \"npm run watch-ts\" \"npm run watch-node\""
  },
  "dependencies": {
    "@discordjs/voice": "^0.18.0",
    "@types/express": "^5.0.1",
    "@types/node-fetch": "^2.6.12",
    "apify-client": "^2.12.1",
    "axios": "^1.8.4",
    "cheerio": "^1.0.0",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "fast-xml-parser": "^5.2.0",
    "ffmpeg-static": "^5.2.0",
    "libsodium-wrappers": "^0.7.15",
    "node-fetch": "^2.7.0",
    "puppeteer": "^24.6.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^22.14.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
